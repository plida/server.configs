server {
	listen 80;
	add_header x-who howto.yetti;
	server_name _;
	index index.php;
	root /home/prj/project/public;
	set $php_sock unix:/var/run/php/php8.3-fpm.sock;
	location / {
		try_files $uri $uri/ /index.php?$query_string;
	}
	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass    $php_sock;
	}
}


